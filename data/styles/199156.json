  "style": {
    "css": "@-moz-document domain(\"cppreference.com\") {\r\n    div#cpp-head-first,div#cpp-head-second,div#content,div#footer {\r\n        -moz-box-sizing: content-box !important;\r\n        box-sizing: content-box !important;\r\n    }\r\n}"
