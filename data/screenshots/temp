    <script>
        const pageLoadedAt = new Date().getTime();
        const debugMode = localStorage.getItem("debugMode");
        const sendPerformance = debugMode || Math.random() < (1/5); // count time to 1 every 5 users
        let userState = sessionStorage.getItem("userState");
        userState =  userState ? 'old' : 'new';
        sessionStorage.setItem('userState', userState);
        window.msSincePageLoad = ()=>{
            return new Date().getTime() - pageLoadedAt;
        };
        window.timingGA = (label, value) =>{
            if (sendPerformance){
                window.ga('send', 'event', 'timing_new', userState, label, value);
            }
        };
    </script>
  <script>
      window.addEventListener("message", (event) => {
          if (event.source == window &&
              event.data.direction &&
              event.data.direction === "from-content-script" && event.data.message === "StylishInstalled") {
              window.isInstalled = true;
          }
      });
  </script>

